</head>
<body>

<div class="header">
  <div class="container main">
    <div class="row">
      <div class="header_top">
        <div class="currency_translator span5">
          <div class="currency">
            CURRENCY:
            <select class="select_bg">
              <option value="dollar">DOLLAR</option>
              <option value="euro">EURO</option>
              <option value="taka">TAKA</option>
              <option value="rupe">RUPE</option>
            </select>
          </div>
          <ul class="translator">
            <li class="en"><a class="active" href="#">EN</a></li>
            <li class="en"><a href="#">ES</a></li>
            <li class="en"><a href="#">FR</a></li>
          </ul>
        </div>
        <div class="account_login span7">

          <ul class="account_info">
            <li><a href="#"><img alt="" src="/assets/my_account.png"> MY ACCOUNT</a></li>
            <li><a href="#"><img alt="" src="/assets/mywish_list.png"> MY WISHLIST</a></li>
            <li><a href="cart.html"><img alt="" src="/assets/shopping_cart.png"> MY CART</a></li>
            <li><a href="checkout.html"><img alt="" src="/assets/checkout.png"> MY CHECKOUT</a></li>
            <% unless current_user %>
                <li><a href="<%= new_user_session_path %>"><img alt="" src="/assets/log_out.png"> SIGN UP / SIGN IN</a></li>
            <% else %>
                <li><a href="<%= url_for(destroy_user_session_path) %>" data-method="delete"><img alt="" src="/assets/log_out.png"> SIGN OUT</a></li>
            <% end %>
          </ul>

        </div>
      </div>
    </div>
    <div class="row">
      <div class="header_top">
        <div class="span4">
          <a href="index-2.html">
            <div class="logo"></div>
          </a>
        </div>
        <div class="call_info span4">
          <p>PHONE: <span class="color">1 (234) 56 78 901</span></p>
          <p>Call us Monday - Saturday: 9:00 am - 6:00 pm</p>
        </div>
        <div class="cart_info span3">
          <div class="cart_data">Shopping Cart - <%= number_to_currency @cart_total %></div>
          <button class="cart_btn btn btn-navbar"><img alt="" src="/assets/shopping_cart_btn_img.png"></button>
